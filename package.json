{
  "name": "seneca-entity",
  "description": "Entity plugin for seneca",
  "keywords": [
    "seneca",
    "entity",
    "plugin"
  ],
  "version": "5.2.0",
  "license": "MIT",
  "homepage": "http://senecajs.org",
  "author": "Richard Rodger (http://richardrodger.com/)",
  "contributors": [
    "Richard Rodger (https://github.com/rjrodger)",
    "Mihai Dima (https://github.com/mihaidma)",
    "Dean McDonnell (https://github.com/mcdonnelldean)",
    "Wyatt Preul (https://github.com/geek)",
    "Shane Lacey (https://github.com/shanel262)",
    "Emer Rutherford (https://github.com/eeswr)",
    "NicolÃ¡s Fantone (https://github.com/nfantone)"
  ],
  "main": "entity.js",
  "files": [
    "LICENSE",
    "README.md",
    "lib",
    "entity.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/senecajs/seneca-entity.git"
  },
  "scripts": {
    "test": "lab -v -t 80",
    "coveralls": "lab -s -P test -r lcov | coveralls",
    "coverage": "lab -v -P test -t 80 -r html -I URL,URLSearchParams > coverage.html",
    "prettier": "prettier --write --no-semi --single-quote entity.js lib/*.js test/*.js",
    "api-docs": "doxdox '*/**/*.js' --layout markdown --output API.md",
    "clean-npm": "rm -rf node_modules package-lock.json",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: v$REPO_VERSION && git commit -a -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags;",
    "repo-publish": "npm run clean-npm && npm i --registry=http://registry.npmjs.org && npm run prettier && npm test && npm run repo-tag && npm publish --access public --registry=http://registry.npmjs.org"
  },
  "dependencies": {
    "eraro": "^1.1.0",
    "jsonic": "^0.3.1",
    "nid": "^0.3.2",
    "seneca-mem-store": "^1.6.0"
  },
  "devDependencies": {
    "@hapi/code": "^6.0.0",
    "@hapi/joi": "^15.1.1",
    "@hapi/lab": "^20.3.2",
    "async": "^3.1.0",
    "coveralls": "^3.0.6",
    "gex": "^0.3.0",
    "prettier": "^1.18.2",
    "seneca": "^3.16.1",
    "seneca-plugin-validator": "0.4.0"
  }
}
