{
  "name": "seneca-entity",
  "description": "Entity plugin for seneca",
  "keywords": [
    "seneca",
    "entity",
    "plugin"
  ],
  "version": "1.2.0",
  "license": "MIT",
  "homepage": "http://senecajs.org",
  "author": "Richard Rodger (http://richardrodger.com/)",
  "main": "entity.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib",
    "entity.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/senecajs/seneca-entity.git"
  },
  "scripts": {
    "test": "lab -v -L -t 74 $LAB_FLAGS",
    "test:node6": "LAB_FLAGS='--leaks' && npm test",
    "test-html": "lab -v -L -t 80 -r html > testcov.html",
    "coverage": "lab test -v -r lcov | coveralls",
    "lint": "lab -dL"
  },
  "dependencies": {
    "eraro": "0.4.1",
    "jsonic": "0.2.2",
    "lodash": "4.14.2",
    "seneca-mem-store": "0.5.1"
  },
  "devDependencies": {
    "async": "2.0.x",
    "code": "3.0.x",
    "coveralls": "2.11.x",
    "eslint-config-seneca": "3.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "eslint-plugin-standard": "2.x.x",
    "gex": "0.2.2",
    "joi": "9.0.x",
    "lab": "11.0.x",
    "pre-commit": "1.1.3",
    "seneca": "plugin"
  },
  "pre-commit": [
    "test",
    "lint"
  ]
}
